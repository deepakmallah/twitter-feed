{
  "name": "boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --inline",
    "build": "rm -rf dist && NODE_ENV=production webpack -p",
    "build:withSSR": "rm -rf dist && SSR=1 NODE_ENV=production webpack -p",
    "webpack:stats": "rm -rf dist && NODE_ENV=production webpack -p --profile --json > webpack-stats.json",
    "serve:dist": "superstatic dist --port 8080",
    "start:prod": "node dist/server.js",
    "lint": "eslint --ext .js --ext .jsx",
    "lint:fix": "eslint --ext .js --ext .jsx  --fix",
    "docker:build": "docker-compose build",
    "docker:start": "docker-compose up",
    "docker:stop": "docker-compose down -v",
    "test": "jest --config jest.config.json"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.0",
    "babel-core": "^6.22.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.26.0",
    "html-webpack-template": "^6.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-loader": "^1.2.2",
    "postcss-smart-import": "^0.6.7",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "style-loader": "^0.13.1",
    "superstatic": "^4.0.3",
    "webpack": "^2.2.0",
    "webpack-dev-server": "2.2.0",
    "webpack-hot-middleware": "^2.15.0"
  },
  "dependencies": {
    "antd": "^2.6.4",
    "babel-plugin-import": "^1.1.0",
    "body-parser": "^1.18.2",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "firebase": "^4.6.2",
    "mongoose": "^4.13.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-router": "^3.0.2",
    "superagent": "^3.8.1",
    "twitter": "^1.7.1"
  }
}
